{
  "name": "gulp-ssr-tpl",
  "version": "1.0.0",
  "description": "Template for web app based on gulp, express and Vue",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build:dev": "gulp build",
    "build:prod": "NODE_ENV=production gulp build",
    "stream": "gulp autoreload --task watch",
    "node": "nodemon -L server.js",
    "serve": "run-p node stream",
    "stack": "docker-compose -f stack.yml up -d",
    "dev": "run-s build:dev stack serve",
    "test": "run-s build:prod stack start"
  },
  "author": "Uhomuho",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.27.7",
    "clean-css": "^5.2.4",
    "cross-spawn-with-kill": "^1.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-hash-filename": "^4.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-purgecss": "^4.1.3",
    "gulp-sass": "^5.1.0",
    "gulp-webp": "^4.0.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "sass": "^1.49.9",
    "through2": "^4.0.2",
    "vue": "^3.2.31",
    "webpack-stream": "^7.0.0",
    "yargs": "^17.3.1"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "module-alias": "^2.2.2",
    "pg": "^8.7.3",
    "pug": "^3.0.2",
    "rand-token": "^1.0.1",
    "sequelize": "^6.17.0"
  }
}
